{
  "manifest_version": 2,

  "name": "You Ain't Gonna Need All Those Tabs",
  "description": "This extension closes tabs that haven't been opened in a while.",
  "version": "1.0",

  "minimum_chrome_version": "40",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "YAGNATT"
  },

  "permissions": [
    "activeTab",
    "history",
    "storage",
    "tabs"
  ]
}
